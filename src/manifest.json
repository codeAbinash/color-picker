{
    "manifest_version": 2,
    "name": "Color Picker",
    "version": "1.0.0",
    "description": "A quick color picker extension",
    "icons": {
        "128": "./icons/icon128.png",
        "48": "./icons/icon48.png",
        "16": "./icons/icon16.png"
    },
    "browser_action": {
        "default_icon": "./icons/icon16.png",
        "default_popup": "index.html"
    },
    "options_page": "options.html"
}